   3.944048303589302*^9},ExpressionUUID->"6dda4730-1d98-104c-a2fc-\
5bca0a137940"],

Cell[BoxData[
 RowBox[{
  RowBox[{"fblender", "[", 
   RowBox[{"col1_", ",", "col2_"}], "]"}], ":=", 
  RowBox[{"RGBColor", "@@", 
   RowBox[{"Echo", "@", 
    RowBox[{"MapThread", "[", 
     RowBox[{"BitOr", ",", 
      RowBox[{"Echo", "@", 
       RowBox[{"MapApply", "[", 
        RowBox[{"List", ",", 
         RowBox[{"{", 
          RowBox[{"col1", ",", "col2"}], "}"}]}], "]"}]}]}], 
