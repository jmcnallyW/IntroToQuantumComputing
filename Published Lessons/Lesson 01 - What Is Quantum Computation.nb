 RowBox[{"fourbits", "=", 
  RowBox[{
   RowBox[{"Tuples", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\"\<0\>\"", ",", "\"\<1\>\""}], "}"}], ",", "4"}], "]"}], "//", 
   RowBox[{"Map", "[", "StringJoin", "]"}]}]}]], "Input",
 CellChangeTimes->{{3.943975768535692*^9, 3.943975801177696*^9}, {
  3.9439759125306797`*^9, 3.943975914883465*^9}},
